# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
end_of_line = lf
insert_final_newline = true

[*.py]
charset = utf-8
indent_style = space
indent_size = 4
max_line_length = 88

[*.json]
indent_style = space
indent_size = 4

[Makefile]
indent_style = space
